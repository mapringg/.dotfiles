{
  "assistant": {
    "default_model": {
      "model": "claude-3-5-sonnet-latest",
      "provider": "zed.dev"
    },
    "version": "2"
  },
  "buffer_font_family": "MonoLisa",
  "buffer_font_size": 12,
  "buffer_line_height": {
    "custom": 1.5
  },
  "file_types": {
    "CSS": ["scss"]
  },
  "ssh_connections": [
    {
      "host": "dev",
      "projects": [
        {
          "paths": ["~/influally-client-staging"]
        }
      ]
    },
    {
      "host": "pre",
      "projects": [
        {
          "paths": ["~/influencer-client"]
        }
      ]
    },
    {
      "host": "pro",
      "projects": [
        {
          "paths": ["~/influencer-client"]
        }
      ]
    }
  ],
  "ui_font_size": 13
}
