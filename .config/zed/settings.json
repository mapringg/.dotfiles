{
  "assistant": {
    "default_model": {
      "model": "claude-3-5-sonnet-latest",
      "provider": "zed.dev"
    },
    "version": "2"
  },
  "buffer_font_family": "MonoLisa",
  "buffer_font_size": 13,
  "buffer_line_height": "standard",
  "current_line_highlight": "none",
  "cursor_blink": false,
  "file_types": {
    "CSS": ["scss"]
  },
  "indent_guides": {
    "enabled": false
  },
  "inlay_hints": {
    "enabled": true
  },
  "preview_tabs": {
    "enabled": false,
    "enable_preview_from_code_navigation": false
  },
  "project_panel": {
    "show_diagnostics": "all",
    "scrollbar": {
      "show": "never"
    }
  },
  "ssh_connections": [
    {
      "host": "dev",
      "projects": [
        {
          "paths": ["~/influally-client-staging"]
        }
      ]
    }
  ],
  "terminal": {
    "line_height": "standard"
  },
  "theme": "Gruvbox Dark Hard",
  "ui_font_size": 13,
  "use_autoclose": false,
  "vim_mode": true,
  "wrap_guides": [80]
}
