Host *
  IgnoreUnknown UseKeychain
  AddKeysToAgent yes
  UseKeychain yes
  ForwardAgent yes
  ExitOnForwardFailure no
  SetEnv TERM=xterm-256color

Host pre
  IdentityFile ~/.ssh/zanroo
  User root
  HostName 172.16.200.70

Host dev
  IdentityFile ~/.ssh/zanroo
  User root
  HostName 172.16.200.71

Host pro
  IdentityFile ~/.ssh/zanroo
  User root
  HostName 172.16.200.72
